$TTL    604800
@       IN      SOA     services.okd.local. admin.okd.local. (
                  1     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800     ; Negative Cache TTL
)

; name servers - NS records
    IN      NS      services

; name servers - A records
services.okd.local.          IN      A       10.1.1.128

; OpenShift Container Platform Cluster - A records
bootstrap.okd.local.          IN      A      10.1.1.200
control-plane.okd.local.      IN      A      10.1.1.201
compute-1.okd.local.          IN      A      10.1.1.202
compute-2.okd.local.          IN      A      10.1.1.203

; OpenShift internal cluster IPs - A records
api.okd.local.        IN    A    10.1.1.200
api-int.okd.local.    IN    A    10.1.1.200
*.apps.okd.local.     IN    A    10.1.1.200
etcd-0.okd.local.     IN    A    10.1.1.201
etcd-1.okd.local.     IN    A    10.1.1.202
etcd-2.okd.local.     IN    A    10.1.1.203
openshift.apps.okd.local.   IN     A     10.1.1.200
oauth.apps.okd.local.       IN     A     10.1.1.200

; OpenShift internal cluster IPs - SRV records
_etcd-server-ssl._tcp.okd.local.    86400     IN    SRV     0    10    2380    etcd-0
_etcd-server-ssl._tcp.okd.local.    86400     IN    SRV     0    10    2380    etcd-1
_etcd-server-ssl._tcp.okd.local.    86400     IN    SRV     0    10    2380    etcd-2

